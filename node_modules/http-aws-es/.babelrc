{
  "env": {
    "node4": {
      "presets": [
        ["env", {
          "targets": { "node": "4" }
        }]
      ],
      "plugins": [
        ["transform-runtime", {
          "polyfill": false,
          "regenerator": true
        }]
      ]
    },
    "node6": {
      "presets": [
        ["env", {
          "targets": { "node": "6" }
        }]
      ]
    },
    "node8": {
      "presets": [
        ["env", {
          "targets": { "node": "8" }
        }]
      ]
    },
    "legacy": {
      "presets": ["env"],
      "plugins": [
        ["transform-runtime", {
          "polyfill": true,
          "regenerator": true
        }]
      ]
    }
  }
}
